{
  "name": "timer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=dev vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.4.4",
    "cross-env": "^7.0.3",
    "crossenv": "^0.0.2-security",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.10",
    "vue-inline-svg": "^1.3.0",
    "vue-masked-input": "^0.5.2",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.3.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/require-v-for-key": "off",
      "vue/no-console": "off",
      "no-unused-vars": "off",
      "no-redeclare": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}